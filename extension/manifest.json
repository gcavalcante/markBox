{
  "name": "MarkBox",
  "version": "1.0",
  "description": "Syncs bookmarks with your Facebook Groups",
  "icons": {
    "128": "icon128.png"
  },

  "background": { "scripts": ["assets/js/jquery-1.8.3.min.js", "oauth2/oauth2.js", "background.js"] },
  "browser_action": {
    "default_title": "OAuth 2.0",
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
      "bookmarks",
      "tabs",
      "https://graph.facebook.com/",
      "http://amigonerd.cloudapp.net/*"
  ],
  "manifest_version": 2
}
